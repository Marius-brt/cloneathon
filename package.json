{
  "name": "cloneathon",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "bun run src/scripts/load-models.ts && next build --turbopack && mkdir -p .next/standalone/src/assets && cp src/assets/models.json .next/standalone/src/assets/models.json",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:push": "prisma db push",
    "db:studio": "prisma studio",
    "dev": "next dev --turbopack",
    "format": "sort-package-json package.json  && biome format --write ./",
    "lint": "biome check --organize-imports-enabled=true ./",
    "load-models": "bun run src/scripts/load-models.ts",
    "start": "next start"
  },
